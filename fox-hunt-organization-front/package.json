  "scripts": {
    "start": "npm run build-common && react-scripts start",
    "start:dev": "react-scripts start",
    "build-common": "cd ../common-front/bin && npm run compile && cd ../../fox-hunt-organization-front",
    "docker-local": "npm i && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preinstall": "yalc update",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "prepare": "cd .. && husky install .husky",
    "storybook": "start-storybook -p 6007 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
